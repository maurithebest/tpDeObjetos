import example.*
describe "group of tests" {
	const laFamilia
	const cisne
	const joaquin
	const lucia
	const especialLaFamilia
	const laSole
	const eres
	const corazonAmericano
	const remixLaFamilia
	const mashupA
	const crisantemo
	const almaDeDiamante
	const pdepalooza
	const lunaPark
	const paraLosArboles
	const justCrisantemo
	const luisAlberto
	const joaquin2
	const aliciaEnElPaisDeLasMaravillas
	
	
	fixture{
		joaquin2= new Artista("joaquin",new Solista("familia"),20,new CantidadEnGrupo(),[especialLaFamilia])	
		
		laFamilia = new Cancion("la familia",264,["quiero","brindar","por","mi","gente","sencilla","por","el","amor","brindo","por","la","familia"])
		cisne = new Cancion("cisne", 312,["hoy", "el" ,"viento", "se" ,"abrió", "quedó" ,"vacío", "el" ,"aire", "una", "vez", "más"," y"," el"," manantial"," brotó"," y"," nadie", "está", "aquí", "y" ,"puedo" ,"ver" ,"que" ,"solo" ,"estallan" ,"las" ,"hojas" ,"al", "brilla"])
		eres = new Cancion("eres",145,["eres", "lo", "mejor", "que", "me", "pasó", "en", "la", "vida", "no", "tengo", "duda", "no", "habrá", "más", "nada", "después", "de", "ti","eres", "lo", "que", "le", "dio", "brillo", "al", "día", "a", "día", "y", "así", "será", "por", "siempre", "no", "cambiará", "hasta", "el", "final", "de", "mis", "días"])
		corazonAmericano = new Cancion("corazon americano",154,["canta", "corazón", "canta", "más", "alto", "que", "tu", "pena", "al", "fin", "se", "va", "marchando", "el", "nuevo", "milenio", "ha", "de", "encontrarnos", "junto", "corazón", "como", "soñamos"]) 
		remixLaFamilia= new Remix("la familia",264,["quiero","brindar","por","mi","gente","sencilla","por","el","amor","brindo","por","la","familia"],laFamilia)
		almaDeDiamante = new Cancion("almaDeDiamante",216,["ven", "a", "mí", "con", "tu", "dulce", "luz", "alma", "de", "diamante", "y", "aunque", "el", "sol", "se", "nuble", "después", "sos", "alma", "de", "diamante", "cielo", "o", "piel", "silencio", "o", "verdad", "sos", "alma", "de", "diamante", "por", "eso", "ven", "así", "con", "la", "humanidad" ,"alma", "de", "diamante"])
		crisantemo = new Cancion("crisantemo",175,["tócame", "junto", "a", "esta", "pared", "yo", "quede", "por", "aquí...", "cuando", "no", "hubo", "más", "luz...", "quiero", "mirar", "a", "través", "de", "mi", "piel...", "crisantemo", "que", "se", "abrió...", "encuentra", "el", "camino", "hacia", "el", "cielo"])
		paraLosArboles = new Album("para los arboles",31,3,2003,[cisne,almaDeDiamante],50000,49000)
		justCrisantemo = new Album("just crisantemo",5,12,2007,[crisantemo],28000,27500)
		especialLaFamilia = new Album("especial la familia", 17,6,1992,[laFamilia],100000,89000)
		laSole = new Album("la Sole",4,2,2005,[eres, corazonAmericano],200000,130000)
		joaquin= new Artista("joaquin",new MusicoDeGrupo(5),20,new CantidadEnGrupo(),[especialLaFamilia])	
		lucia = new Artista("lucia",new Solista("familia"),70,new PorCapacidad(),[])
		mashupA= new Mashup("mashupA",121,["hola"],[almaDeDiamante,crisantemo])
		aliciaEnElPaisDeLasMaravillas = new Cancion("alicia en el pais de las maravillas",510,["quién", "sabe", "alicia", "este", "país", "no", "estuvo", "hecho", "porque", "sí", "te", "vas", "a", "ir", "vas", "a", "salir", "pero", "te", "quedas", "¿dónde", "más", "vas", "a", "ir?", "y", "es", "que", "aquí", "sabes", "el", "trabalenguas", "trabalenguas", "el", "asesino", "te", "asesina", "y", "es", "mucho", "para", "ti", "se", "acabó", "ese", "juego", "que", "te", "hacía", "feliz"])
		
		luisAlberto = new Artista("luis Alberto",new LuisAlberto(fender),0,new FechaConcierto(),[paraLosArboles,justCrisantemo])
		lunaPark = new Lugar([luisAlberto,lucia,joaquin],20,4,2017,9290)
		//laTrastienda =new Lugar([luisAlberto,lucia,joaquin],15,11,2017,400)
		//kike = new Cantante(new EnGrupo(20),60,new DuraMasDe300Segundos(),new CantidadEnGrupo(),[])
		pdepalooza= new Presentacion(lunaPark,15,12,2017,[new CriterioPorHabilidad(70),new CriterioPorObra(),new CriterioPorCancion(aliciaEnElPaisDeLasMaravillas)])
		//soledad = new Cantante(new Solista(),55,new TienePalabraEspecificaLaCancion("amor"),new CantidadEnGrupo(),[laSole])
	}
	test "Queremos saber si Joaquín canta bien la canción “La Familia" {

		assert.that(joaquin.interpretaBien(laFamilia))

	}
	test "Queremos saber si Joaquín canta bien la canción “Cisne”" {

		assert.that(joaquin.interpretaBien(cisne))

	}
	test "Queremos saber si Lucía canta bien la canción “La Familia” como solista" {

		assert.that(lucia.interpretaBien(laFamilia))

	}
	test "Queremos saber si Lucía canta bien la canción “Cisne” como solista" {

		assert.that(lucia.interpretaBien(cisne))

	}
	test "Queremos saber si Luis Alberto canta bien la canción “La Familia”" {
		
		assert.that(luisAlberto.interpretaBien(laFamilia))

	}
	test "Queremos saber si Luis Alberto canta bien la canción “Cisne”" {
		
		assert.that(luisAlberto.interpretaBien(cisne))

	}
	test "Saber cuál es la canción más larga del álbum “Especial La Familia”, teniendo en cuenta la duración" {
		
		assert.equals(especialLaFamilia.cancionMasDuradera(),laFamilia)

	}
	test "Saber cuál es la canción más larga del álbum “La Sole”, teniendo en cuenta la duración" {
		
		assert.equals(laSole.cancionMasDuradera(),corazonAmericano)

	}
	test "Saber cuál es la canción más larga del álbum “La Sole”, teniendo en cuenta la longitud de la letra" {
		
		assert.equals(remixLaFamilia.duracion(),792)

	}
	test "Saber cuál es la letra del remix de “La Familia”" {
		
		assert.equals(remixLaFamilia.letra(),"mueve tu cuelpo baby quiero brindar por mi gente sencilla por el amor brindo por la familia yeah oh yeah")

	}
	test "Saber cuál es la duración del mashup de “Alma de Diamante” y  “Crisantemo”" {
		
		assert.equals(mashupA.canciones(),[almaDeDiamante,crisantemo])

	}
	test "Saber cuál es la letra del mashup entre “Alma de Diamante” y “Crisantemo”" {
		
		assert.equals(mashupA.letra(),"ven a mí con tu dulce luz alma de diamante y aunque el sol se nuble después sos alma de diamante cielo o piel silencio o verdad sos alma de diamante por eso ven así con la humanidad alma de diamante tócame junto a esta pared yo quede por aquí... cuando no hubo más luz... quiero mirar a través de mi piel... crisantemo que se abrió... encuentra el camino hacia el cielo")

	}
	
	test "Intentar agregar al recital Pdpalooza a Luis Alberto" {
		pdepalooza.agregarMusico(luisAlberto)
		assert.equals(pdepalooza.musicos(), [luisAlberto])
	}
	test "Intentar agregar al recital Pdpalooza a Joaquín como solista" {
		
		pdepalooza.agregarMusico(joaquin2)
		assert.throwsExceptionWithType(new NoCumpleCriterio(), {pdepalooza.agregarMusico(joaquin2)})
	}
}