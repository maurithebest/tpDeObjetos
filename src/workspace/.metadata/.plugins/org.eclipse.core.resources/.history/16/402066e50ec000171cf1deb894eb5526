import example.*
describe "group of tests" {
	const laFamilia
	const cisne
	const joaquin
	const lucia
	const especialLaFamilia
	const laSole
	const eres
	const corazonAmericano
	const remixLaFamilia
	const mashupA
	const crisantemo
	const almaDeDiamante
	const pdepalooza
	
	const paraLosArboles
	const justCrisantemo
	const luisAlberto
	//const joaquin2
	const aliciaEnElPaisDeLasMaravillas
	const soledad
	//const lucia2
	const kike
	const prixDAm
	const laCueva
	const pimpisole
	const cachorro
	const felix
	const elEternoEntorno
	fixture{
		//joaquin2= new Artista("joaquin",new Solista("familia"),20,new CantidadEnGrupo(),[especialLaFamilia])	
		
		laFamilia = new Cancion("la familia",264,["quiero","brindar","por","mi","gente","sencilla","por","el","amor","brindo","por","la","familia"])
		cisne = new Cancion("cisne", 312,["hoy", "el" ,"viento", "se" ,"abrió", "quedó" ,"vacío", "el" ,"aire", "una", "vez", "más"," y"," el"," manantial"," brotó"," y"," nadie", "está", "aquí", "y" ,"puedo" ,"ver" ,"que" ,"solo" ,"estallan" ,"las" ,"hojas" ,"al", "brilla"])
		eres = new Cancion("eres",145,["eres", "lo", "mejor", "que", "me", "pasó", "en", "la", "vida", "no", "tengo", "duda", "no", "habrá", "más", "nada", "después", "de", "ti","eres", "lo", "que", "le", "dio", "brillo", "al", "día", "a", "día", "y", "así", "será", "por", "siempre", "no", "cambiará", "hasta", "el", "final", "de", "mis", "días"])
		corazonAmericano = new Cancion("corazon americano",154,["canta", "corazón", "canta", "más", "alto", "que", "tu", "pena", "al", "fin", "se", "va", "marchando", "el", "nuevo", "milenio", "ha", "de", "encontrarnos", "junto", "corazón", "como", "soñamos"]) 
		remixLaFamilia= new Remix("la familia",264,["quiero","brindar","por","mi","gente","sencilla","por","el","amor","brindo","por","la","familia"],laFamilia)
		almaDeDiamante = new Cancion("almaDeDiamante",216,["ven", "a", "mí", "con", "tu", "dulce", "luz", "alma", "de", "diamante", "y", "aunque", "el", "sol", "se", "nuble", "después", "sos", "alma", "de", "diamante", "cielo", "o", "piel", "silencio", "o", "verdad", "sos", "alma", "de", "diamante", "por", "eso", "ven", "así", "con", "la", "humanidad" ,"alma", "de", "diamante"])
		crisantemo = new Cancion("crisantemo",175,["tócame", "junto", "a", "esta", "pared", "yo", "quede", "por", "aquí...", "cuando", "no", "hubo", "más", "luz...", "quiero", "mirar", "a", "través", "de", "mi", "piel...", "crisantemo", "que", "se", "abrió...", "encuentra", "el", "camino", "hacia", "el", "cielo"])
		paraLosArboles = new Album("para los arboles",31,3,2003,[cisne,almaDeDiamante],50000,49000)
		justCrisantemo = new Album("just crisantemo",5,12,2007,[crisantemo],28000,27500)
		especialLaFamilia = new Album("especial la familia", 17,6,1992,[laFamilia],100000,89000)
		laSole = new Album("la Sole",4,2,2005,[eres, corazonAmericano],200000,130000)
		joaquin= new Artista("joaquin",new MusicoDeGrupo(5),20,new CantidadEnGrupo(100),[especialLaFamilia],new Largueros(300))	
		lucia = new Artista("lucia",new Solista("familia"),70,new PorCapacidad(5000,500),[],new Palabreros("familia"))
		//lucia2 = new Artista("lucia",new Solista("familia"),90,new PorCapacidad(),[])
		cachorro=new Representante(1982)
		felix=new Representante(500)
		luisAlberto = new Artista("luis Alberto",new LuisAlberto(fender),0,new FechaConcierto(1000,30,9,2017,1.2),[paraLosArboles,justCrisantemo],new Impareros())
		kike = new Artista("kike",new MusicoDeGrupo(20),60,new FechaConcierto(4000,8,1,2018,0.5),[],new Impareros())
		
		mashupA= new Mashup("mashupA",121,["hola"],[almaDeDiamante,crisantemo])
		aliciaEnElPaisDeLasMaravillas = new Cancion("alicia en el pais de las maravillas",510,["quién", "sabe", "alicia", "este", "país", "no", "estuvo", "hecho", "porque", "sí", "te", "vas", "a", "ir", "vas", "a", "salir", "pero", "te", "quedas", "¿dónde", "más", "vas", "a", "ir?", "y", "es", "que", "aquí", "sabes", "el", "trabalenguas", "trabalenguas", "el", "asesino", "te", "asesina", "y", "es", "mucho", "para", "ti", "se", "acabó", "ese", "juego", "que", "te", "hacía", "feliz"])
		pimpisole=new Grupo("pimpisole",[soledad,joaquin,lucia],cachorro,[])
		elEternoEntorno=new Grupo("elEternoEntorno",[kike,luisAlberto],felix,[])
		
		//laTrastienda =new Lugar([luisAlberto,lucia,joaquin],15,11,2017,400)
		
		
		pdepalooza= new Presentacion(15,12,2017,[new CriterioPorHabilidad(70),new CriterioPorObra(),new CriterioPorCancion(aliciaEnElPaisDeLasMaravillas)],[],9280)
		prixDAm= new Presentacion(30,6,2017,[new CriterioPorHabilidad(70),new CriterioPorObra(),new CriterioPorCancion(aliciaEnElPaisDeLasMaravillas)],[joaquin,soledad],150)
		laCueva= new Presentacion(5,1,2018,[new CriterioPorHabilidad(70),new CriterioPorObra(),new CriterioPorCancion(aliciaEnElPaisDeLasMaravillas)],[pimpisole],14000)
		
		soledad = new Artista("soledad",new Solista("amor"),80,new CantidadEnGrupo(6000),[laSole],new Palabreros("amor"))
	}
	test "Queremos saber si Joaquín canta bien la canción “La Familia”" {
		joaquin.cambiarTipo(new Impareros())
		assert.that(joaquin.interpretaBien(laFamilia))
	}
	test "Queremos saber si Joaquín canta bien la canción “Cisne”." {
		joaquin.cambiarTipo(new Impareros())
		assert.that(joaquin.interpretaBien(cisne))
	}
	test "Hacer que Kike pase a ser palabrero de la palabra “de”. 
Queremos saber de esta lista de canciones: “Corazón americano”, “Eres”, “La familia”, “Cisne”, “Alma de diamante”, “Canción de Alicia en el país” y “Crisantemo” cuáles puede tocar bien.
" {
		kike.cambiarTipo(new Palabreros("de"))
		assert.equals(kike.puedeTocarBien([corazonAmericano, eres, laFamilia, almaDeDiamante, crisantemo]),[corazonAmericano,eres,almaDeDiamante,crisantemo])
	}
	test "Queremos saber cuáles canciones puede tocar bien Joaquín, dada esta lista de canciones: “Corazón americano”, “Eres”, “La familia”, “Cisne”, “Alma de diamante”, “Canción de Alicia en el país” y “Crisantemo”Joaquín pasa a cobrar por expectativa inflacionaria ($ 1000 y a partir de febrero del 2017 un 20% más). Averiguar cuánto cobra por la presentación de Prix D’Ami" {
		
		assert.equals(joaquin.puedeTocarBien([corazonAmericano, eres, laFamilia, cisne, almaDeDiamante, aliciaEnElPaisDeLasMaravillas, crisantemo]),[laFamilia,cisne,aliciaEnElPaisDeLasMaravillas])
	}
	test "Joaquín pasa a cobrar por expectativa inflacionaria ($ 1000 y a partir de febrero del 2017 un 20% más). Averiguar cuánto cobra por la presentación de Prix D’Ami" {
		
		joaquin.cambiarCriterioDeCobro(new FechaConcierto(1000,1,2,2017,1.2))
		assert.equals(joaquin.cobrar(prixDAm),1200)
		}
	test "Cambiar la fecha de Prix D’Ami para el 24/01/2017.
Cambiar a Joaquín para que cobre por expectativa inflacionaria ($ 1000 y a partir de febrero del 2017 un 20% más). Averiguar cuánto cobra por la presentación de Prix D’Ami
" {
		prixDAm.cambiarFecha(24,1,2017)
		joaquin.cambiarCriterioDeCobro(new FechaConcierto(1000,1,2,2017,1.2))
		assert.equals(joaquin.cobrar(prixDAm),1000)
		}
	test "Averiguar cuánto cobra Joaquín por la presentación de Prix D’Ami" {
		
		
		assert.equals(joaquin.cobrar(prixDAm),50)
		}
	test "Cambiar a Joaquín para que cobre $ 2.500 por presentaciones con capacidad de hasta 13.000.
Averiguar cuánto cobra por la presentación de Prix D’Ami
" {
		
		joaquin.cambiarCriterioDeCobro(new PorCapacidad(13000,2500))
		assert.equals(joaquin.cobrar(prixDAm),2400)
		}
	test "Cambiar a Joaquín para que cobre $ 2.500 por capacidad de 13.000.
Averiguar cuánto cobra por la presentación de La Cueva
" {
		
		joaquin.cambiarCriterioDeCobro(new PorCapacidad(13000,2500))
		assert.equals(joaquin.cobrar(laCueva),2500)
		}
	test "Saber cuánto cobra el grupo “El eterno retorno” por el show de Prix D’Ami" {
		
		
		assert.equals(elEternoEntorno.nombre(),"elEternoEntorno")
		}
		
	
}